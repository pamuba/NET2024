<Window x:Class="CLIENT_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CLIENT_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Closed="Window_Closed">
    <Grid>
        <StackPanel>
            <TextBox Name="firstTB" HorizontalAlignment="Left" Margin="474,93,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
            <TextBox Name="secondTB" HorizontalAlignment="Left" Margin="474,93,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
            <TextBox Name="thirdTB" HorizontalAlignment="Left" Margin="474,93,0,0" TextWrapping="Wrap" Text="TextBox" Width="120" Height="26"/>
            <Button Content="ADD" Click="Button_Click"/>
            <Button Content="SUBTRACT" Click="Button_Click_1"/>
            <Button Content="Print Files" Click="Button_Click_2"/>
        </StackPanel>
    </Grid>
</Window>
