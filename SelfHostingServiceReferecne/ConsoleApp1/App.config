<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="WCFLibrary.MathService" behaviorConfiguration="ServiceBehavior">
        <endpoint address="net.tcp://localhost:6666/MathService"
                  binding="netTcpBinding"
                  name="MathService_netTcpBinding"
                  contract="WCFLibrary.IMathService"/>

        <endpoint address="mex"
                  contract="IMetadataExchange"
                  binding="mexTcpBinding"/>

        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:6667/"/>
          </baseAddresses>
        </host>

      </service>
    </services>

    <behaviors>
      <serviceBehaviors>
        <behavior name="ServiceBehavior">
          <serviceMetadata httpGetEnabled="false"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>


  </system.serviceModel>
</configuration>